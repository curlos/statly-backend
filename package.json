{
  "name": "statly-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "dev:local": "nodemon --exec ts-node -r dotenv/config src/app.ts dotenv_config_path=.env.local",
    "dev:local:debug": "nodemon --exec \"node --inspect --require ts-node/register -r dotenv/config src/app.ts\" dotenv_config_path=.env.local",
    "dev:cloud": "nodemon --exec ts-node src/app.ts",
    "dev:debug": "nodemon",
    "prod": "npm run build && npm run start",
    "sync:to-atlas": "chmod +x scripts/sync-to-atlas.sh && ./scripts/sync-to-atlas.sh",
    "sync:from-atlas": "chmod +x scripts/sync-from-atlas.sh && ./scripts/sync-from-atlas.sh",
    "formatAllFilesPrettier": "npx prettier --write '**/*.{ts,tsx}'",
    "removeUnusedMatrixProperties": "nodemon --exec ts-node src/db/removeUnusedMatrixProperties.ts",
    "removeOldHabitLogs": "nodemon --exec ts-node src/db/removeOldHabitLogs.ts",
    "vercel-build": "echo hello"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "^2.0.1",
    "mongoose": "^8.1.3",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/kill-port": "^2.0.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.19",
    "nodemon": "^3.0.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
